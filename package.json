    "@types/node": "20.14.2",
    "@types/qrcode-terminal": "0.12.2",
    "@typescript-eslint/eslint-plugin": "7.13.0",
    "@typescript-eslint/parser": "7.13.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.5",
    "nodemon": "3.1.3",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.1",
    "rimraf": "5.0.7",
